.jeanine
  background #83817e
  position relative
  .description-container
    h1, p, li, span, h2
      color #ffffff !important
  .grid
    z-index 1
    div
      background #474645
  .section
    position relative
    z-index 1
  .section-1
    margin-bottom 90px
    text-align center
    padding-top 120px
    position relative
    img
      display block
      margin 0 auto
    img:first-child
      margin-bottom 120px
      width 33%
    img:last-child
      width 60.2%
  .section-2
    margin-bottom 27%
    z-index 0
    img
      vertical-align top
      &:first-child
        margin 7% 0 0 31%
        position relative
        width 8.59%
        z-index 2
      &:nth-child(2)
        position relative
        width 18%
        z-index 2
      &:nth-child(3)
        left -1.8%
        position relative
        width 22%
      &:nth-child(4)
        left 20%
        position absolute
        top 25%
        width 23%
        z-index 1
      &:nth-child(5)
        bottom -58%
        left 41%
        position absolute
        width 17%
        z-index 0
      &:nth-child(6)
        bottom -63%
        left 58%
        position absolute
        width 11%
        z-index -1
  .section-3
    margin-bottom 120px
    img
      position relative
      width 21.4%
      &:first-child
        position absolute
        right 0
        top 0
        width 15.3%
      &:nth-child(2)
        margin-left 13%
      &:nth-child(3)
        left -4.6%
        width 21.6%
        z-index 1
      &:nth-child(4)
        left -8.6%
        width 18.8%
      &:nth-child(5)
        bottom 1%
        left 57%
        position absolute
        width 28.2%
        z-index -1
  .section-4
    margin-bottom 120px
    img
      display block
      margin 0 auto
      width 48%
  .section-5
    img
      vertical-align middle
      &:first-child
        margin-left 21.6%
        width 14.6%
      &:nth-child(2)
        left -10.6%
        position relative
        width 14.5%
    .box
      display inline-block
      left -8.3%
      position relative
      vertical-align middle
      width 37.3%
      img
        margin 0
        position initial
        width 100%
  .section-6
    img
      display block
      margin 0 auto 120px auto
      width 19.53125%
  .second-section
    background #423936
    padding 120px 0
  .third-section
    background #1e1715
    padding 120px 0
